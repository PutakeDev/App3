<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder' xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	<ui:style>
	</ui:style>

	<g:FlowPanel>
		<g:HorizontalPanel>
			<g:VerticalPanel>
				<g:Grid width="200px" height="100%">
				<g:row>
					<g:customCell><g:Label text="Location Name" wordWrap="false"/></g:customCell>
					<g:customCell><g:Label text="99" ui:field="locationName" horizontalAlignment="ALIGN_LEFT" width="150"/></g:customCell>
				</g:row>
				<g:row>
					<g:customCell><g:Label text="Region" wordWrap="false"/></g:customCell>
					<g:customCell><g:Label text="99" ui:field="locationRegion" horizontalAlignment="ALIGN_LEFT" width="150"/></g:customCell>
				</g:row>				
				<g:row>
					<g:customCell><g:Label text="Street" wordWrap="false"/></g:customCell>
					<g:customCell><g:Label text="99" ui:field="locationStreet" horizontalAlignment="ALIGN_LEFT" width="150"/></g:customCell>
				</g:row>	
				<g:row>
					<g:customCell><g:Label text="City" wordWrap="false"/></g:customCell>
					<g:customCell><g:Label text="99"  horizontalAlignment="ALIGN_LEFT" width="150"/></g:customCell>
				</g:row>
				<g:row>
					<g:customCell><g:Label text="Number of Hives" wordWrap="false"/></g:customCell>
					<g:customCell><g:Label text="99"  horizontalAlignment="ALIGN_LEFT" width="150"/></g:customCell>
				</g:row>
				<g:row>
					<g:customCell><g:Label text="Last Visit" wordWrap="false"/></g:customCell>
					<g:customCell><g:Label text="today (Joerg)" horizontalAlignment="ALIGN_LEFT" width="150"/></g:customCell>
				</g:row>	
																<g:row>
					<g:customCell><g:Label text="Upcoming Tasks" wordWrap="false"/></g:customCell>
					<g:customCell><g:Label text="Many"  horizontalAlignment="ALIGN_LEFT" width="150"/></g:customCell>
				</g:row>	
			</g:Grid>
			
			</g:VerticalPanel>
			
			<g:VerticalPanel>
			
			</g:VerticalPanel>
		</g:HorizontalPanel>
	</g:FlowPanel>
</ui:UiBinder>
